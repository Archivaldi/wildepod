<% include ../../../partials/header %>
<% include ../../../partials/navbar %>


<div class="container">
    <div class="row">
        <div class="col-md-1"><b>Camera name</b></div>
        <div class="col-md-2"><b>Volunteer name</b></div>
        <div class="col-md-2"><b>Camera Update Date</b></div>
        <div class="col-md-1"><b>Status</b></div>
        <div class="col-md-1"><b>Battery level</b></div>
        <div class="col-md-5"><b>Notes</b></div>
    </div>
        <% for (let i = 0; i < histories.length; i++) { %>
            <div class="row">
                <div class="col-md-1"><%= histories[i].camera_name %></div>
                <div class="col-md-2"><%= histories[i].volunteer_name %></div>
                <div class="col-md-2"><%= dates[i]%></div>
                <div class="col-md-2"><%= histories[i].status%></div>
                <div class="col-md-2"><%= histories[i].battery_level%></div>
                <div class="col-md-2"><%= histories[i].notes%></div>
            </div> 
        <% } %>
</div>